<androidx.constraintlayout.motion.widget.MotionLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:id="@+id/page_splash_screen_ek1"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content"
	android:layout_marginStart="0dp"
	android:layout_marginTop="0dp"
	app:layoutDescription="@xml/splash_screen_scene">

	<View
		android:id="@+id/__bg__splash_screen_ek2"
		android:layout_width="443dp"
		android:layout_height="898dp"
		android:background="@drawable/__bg__splash_screen_ek2_shape"
		app:layout_constraintBottom_toBottomOf="parent"
		app:layout_constraintEnd_toEndOf="parent"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

	<TextView
		android:id="@+id/e_mat_ek1"
		android:layout_width="98dp"
		android:layout_height="42dp"
		android:layout_marginTop="378dp"
		android:text="@string/e_mat_ek1_string"
		android:textColor="@color/e_mat_color"
		android:textSize="36sp"
		app:layout_constraintEnd_toEndOf="@+id/__bg__splash_screen_ek2"
		app:layout_constraintStart_toStartOf="parent"
		app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.motion.widget.MotionLayout>








